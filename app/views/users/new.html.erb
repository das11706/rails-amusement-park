<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for(@user) do |f| %>

  <%= f.label :name %><br>
  <%= f.text_field :name %>

  <%= f.label :height %><br>
  <%= f.text_field :height %>

  <%= f.label :happiness %><br>
  <%= f.text_field :happiness %>

  <%= f.label :nausea %><br>
  <%= f.text_field :nausea %>

  <%= f.label :tickets %><br>
  <%= f.text_field :tickets %>

  <%= f.label :admin %><br>
  <%= f.check_box :admin %>

  <%= f.label :password %><br>
  <%= f.text_field :password %>

  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Create User" %>
<% end %>